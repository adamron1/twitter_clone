<h1>JamMan</h1>


<% if current_user %>
<h3>Good to see you, <%= current_user.name %></h3>
  <%= link_to "Check out the scene", posts_path %>
<% else %>
  <h3>What do you wanna do?</h3>
  <%= button_to "Sign Up", new_user_registration_path, method: :get %>
  <%= button_to "Log In", new_user_session_path, method: :get %>
  <%= link_to "Check it out", posts_path %>
<% end %>



